{
  "schemaVersion": 1,
  "id": "stingamod",
  "version": "${version}",

  "name": "Stinga Mod",
  "description": "Adds a sword of the Saviour Stinga the Blessed",
  "authors": [
    "VoicelessBark44"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/stingamod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.stinga.StingaMod"
    ],
    "client": [
      "net.fabricmc.stinga.StingaModClient"
    ],
    "fabric-datagen": [
      "net.fabricmc.stinga.StingaModDataGenerator"
    ]
  },
  "mixins": [
    "stingamod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric-api": "*",
    "minecraft": "~1.19.3",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
